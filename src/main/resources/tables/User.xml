<?xml version="1.0" encoding="UTF-8"?>
<tables xmlns="http://www.migrate.org/schema/tables"  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
   xsi:schemaLocation="http://www.migrate.org/schema/tables tables.xsd">
    <!-- 系统用户数据 -->
    <table type="MIGRATE" to="sys_user" from="twxuser" skip="true">
    	<fields>
			<field to="userid" from="id"></field>    	
    		<field to="account" from="loginname"></field>
    		<field to="fullname" from="name" field_restrict="UNIQUE"></field>
    		<field to="password" from="" field_default="a4ayc/80/OGda4BO/1o/V0etpOqiLx1JwB5S3beHW0s="></field>
    		<!-- 针对无法格式化的数据提供默认值 -->
    		<field to="usertype" from="userType" field_default="1">
    			<format>
    				<format_class_name>org.ipph.format.JsonMethodFormater</format_class_name>
    				<format_class_arg>{'GR':'2','QY':'1','GX':'3'}</format_class_arg>
    			</format>
    			<condition type="ISNOTNULL"></condition>
    		</field>
    		<field to="cardid" from="idcard"></field>
    		<field to="address" from="address"></field>
    		<field to="mobile" from="mobile"></field>
    		<field to="phone" from="tel"></field>
    		<field to="email" from="email"></field>
    		<field to="bankaccount" from="account"></field>
    		<field to="bank" from="bank"></field>
    		<field to="isexpired" from="" field_default="0"></field>
    		<field to="islock" from="" field_default="0"></field>
    		<field to="status" from="" field_default="2"></field>
    		<field to="fromtype" from="" field_default="0"></field>
    		<field to="isimport" from="" field_default="0"></field>
    		<field to="istalent" from="" field_default="0"></field>
    		<field to="submitTime" from="createtime"></field>
    	</fields>
    </table>
    <!-- 专利申请数据的更新 -->
    <table type="UPDATE" to="z_patent" from="tpatentallo" skip="true">
    	<fields>
    		<!-- to有值，from无值作为待更新的字段 ,待更新字段必须在上方-->
    		<field to="isApply" from="" field_default="1"></field>
    		<!-- 两个都有值作为关联条件 -->
    		<field to="appNumber" from="patentNo">
    			<format>
    				<format_class_name>org.ipph.format.PatentNoFormater</format_class_name>
    			</format>
    		</field>
    		<!-- from有值，to无值作为源数据的查询条件 -->
    		<field from="status" to="">
    			<condition type="EQUAL">1</condition>
    		</field>
    		<field to="" from="costtype">
    			<condition type="EQUAL">SQ1</condition>
    		</field>
    		<field to="" from="transfermoney">
    			<condition type="GT">0</condition>
    		</field>
    	</fields>
    </table>
    <!-- 专利授权数据的更新 -->
    <table type="UPDATE" to="z_patent" from="tpatentallo">
    	<fields>
    		<!-- to有值，from无值作为待更新的字段 ,待更新字段必须在上方-->
    		<field to="isCity" from="" field_default="1"></field>
    		<!-- 两个都有值作为关联条件 -->
    		<field to="appNumber" from="patentNo">
    			<format>
    				<format_class_name>org.ipph.format.PatentNoFormater</format_class_name>
    			</format>
    		</field>
    		<!-- from有值，to无值作为源数据的查询条件 -->
    		<field from="status" to="">
    			<condition type="EQUAL">1</condition>
    		</field>
    		<field to="" from="costtype">
    			<condition type="EQUAL">SQ2</condition>
    		</field>
    		<field to="" from="transfermoney">
    			<condition type="GT">0</condition>
    		</field>
    	</fields>
    </table>
    <!-- pct申请数据 -->
    <table type="MIGRATE" to="wf_patent_support_foreign" from="tpatentallo" skip="true">
    	<fields>
			<field to="id" from="id"></field>    	
    		<field to="patentName" from="patentName"></field>
    		<field to="applyNo" from="patentNo">
    			<format>
    				<format_class_name>org.ipph.format.PctNoFormater</format_class_name>
    			</format>
    		</field>
    		<field to="applyDate" from="requestDate"></field>
    		<field to="authDate" from="pctDate"></field>
    		<field to="country" from="requestCountry"></field>
    		<field to="applyer" from="firstRequest"></field>
    		<field to="money" from="transfermoney"></field>
    		<field to="statusType" from="" field_default="1"></field>
    		<field to="type" from="" field_default="0"></field>
    		<field to="" from="status">
    			<condition type="EQUAL">1</condition>
    		</field>
    		<field to="" from="costType">
    			<condition type="EQUAL">SQ5</condition>
    		</field>
    		<field to="" from="transfermoney">
    			<condition type="GT">0</condition>
    		</field>
    	</fields>
    </table>
    <!-- pct授权数据 -->
    <table type="MIGRATE" to="wf_patent_support_foreign" from="tpatentallo" skip="true">
    	<fields>
			<field to="id" from="id"></field>    	
    		<field to="patentName" from="patentName"></field>
    		<field to="authNo" from="patentNo">
    			<format>
    				<format_class_name>org.ipph.format.PctNoFormater</format_class_name>
    			</format>
    		</field>
    		<field to="applyDate" from="requestDate"></field>
    		<field to="authDate" from="pctDate"></field>
    		<field to="country" from="requestCountry"></field>
    		<field to="applyer" from="firstRequest"></field>
    		<field to="money" from="transfermoney"></field>
    		<field to="statusType" from="" field_default="1"></field>
    		<field to="type" from="" field_default="1"></field>
    		<field to="" from="status">
    			<condition type="EQUAL">1</condition>
    		</field>
    		<field to="" from="costType">
    			<condition type="EQUAL">SQ6</condition>
    		</field>
    		<field to="" from="transfermoney">
    			<condition type="GT">0</condition>
    		</field>
    	</fields>
    </table>
    <!-- 软件著作权登记信息 -->
    <table type="MIGRATE" from="tcopyrightreg" to="cs_copyright" skip="true">
    	<fields>
    		<!-- 作品信息 -->
    		<field to="id" from="id"></field>
    		<field to="copyrightNo" from="receiptNo"></field>
    		<field to="copyrightName" from="workname"></field>
    		<field to="copyrightType" from="workType"></field>
    		<field to="copyrightTypeMsg" from="notes"></field>
    		<!-- 作者姓名 -->
    		<field to="authorName" from="authorname"></field>
    		<field to="workSignature" from="worksign"></field>
    		<!-- 作品基本信息 -->
    		<field to="workNature" from="property"></field>
    		<field to="workNatureMsg" from="propertynote"></field>
    		<field to="finishDate" from="overdate"></field>
    		<field to="finishCountry" from="overcountry"></field>
    		<field to="finishCity" from="overcity"></field>
    		<field to="publicState" from="publishtype"></field>
    		<field to="firstPublicDate" from="firstdate"></field>
    		<field to="firstPublicCountry" from="firstcountry"></field>
    		<field to="firstPublicCity" from="firstcity"></field>
    		<!-- 权利状况说明 -->
    		<field to="getRightWay" from="gainway"></field>
    		<field to="RightOwnership" from="belongway"></field>
    		<field to="RightOwnershipMsg" from="belongwaynote"></field>
    		<field to="RightOwnershipStatus" from="allsect"></field>
    		<field to="RightOwnershipParts" from="conditionsect"></field>
    		<field to="RightOwnershipStatusMsg" from="conditionnote"></field>
    		<!-- 留样作品样本 -->
    		<field to="electronicSample" from="electronmedia"></field>
    		<field to="electronicSampleNum" from="electronmedianum" field_default="0"></field>
    		<field to="paperSample" from="papermedia"></field>
    		<field to="paperSampleNum" from="papermedianum" field_default="0"></field>
    		<!-- 申请办理信息 -->
    		<field to="applyType" from="applyway">
    			<condition type="EQUAL">0</condition>
    		</field>
    		<field to="applicantName" from="submitter"></field>
    		<field to="" from="area"></field>
    		<field to="applicantAddress" from="address"></field>
    		<field to="applicantPostcode" from="zip"></field>
    		<field to="applyContacts" from="linkman"></field>
    		<field to="applicantTel" from="tel"></field>
    		<field to="applicantEmail" from="email"></field>
    		<field to="applyContactsPhone" from="mobile"></field>
    		<field to="applicantFax" from="fax"></field>
    		
    		<!-- 工作站 -->
    		<field to="" from="workstation"></field>
    		
    		<field to="" from="status">
    			<condition type="EQUAL">1</condition>
    		</field>
    	</fields>
    	<!-- 字段拆分出来的子表-著作权人子表信息 -->
    	<subTable name="cs_copyright_owner" skip="true">
    		<fields>
    			<!-- <field to="id" from="id">
    				
    			</field> -->
    			<field to="ownerName" from="name">
    				<fieldSeparator>
    					<separator_class_name></separator_class_name>
    					<!-- 分隔符定义为:逗号,分号,\,/,空格以及冒号 -->
    					<separator_class_arg>[,;\/ :]</separator_class_arg>
    				</fieldSeparator>
    				<condition type="ISNOTNULL"></condition>
    			</field>
    			<field to="ownerType" from="type"></field>
    			<field to="ownerIDtype" from="cardType"></field>
    			<field to="ownerIDNum" from="idcard"></field>
    			<field to="ownerNationality" from="country"></field>
    			<field to="ownerProvince" from="province"></field>
    			<field to="ownercity" from="city"></field>
    			<field to="signature" from="sign"></field>
    			<field to="" from="status">
	    			<condition type="EQUAL">1</condition>
	    		</field>
    		</fields>
    	</subTable>
    </table>
    
